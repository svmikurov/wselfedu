# Acceptable boolean values: 'true', 't', '1' for True

# Django
SECRET_KEY=secret
ALLOWED_HOSTS=localhost,127.0.0.1
DEBUG=t
LOGGING=
# Internationalization - available language codes: 'en', 'ru', 'nl'
# Default: 'en' (if not specified)
LANGUAGE_CODE=ru

# Django database connection settings
# (used by Django application)
DB_NAME=wse_db
DB_USER=wse_user
DB_PASS=password
# Database host configuration:
# - Docker deployment: use container name 'wse-postgres'
# - Local development: use 'localhost' or '127.0.0.1'
DB_HOST=localhost
DB_PORT=5432

# PostgreSQL container initialization settings
# (for Docker deployment only)
POSTGRES_DB=
POSTGRES_USER=
POSTGRES_PASSWORD=

# Environment
# Available `DJANGO_ENV`: 'production'
DJANGO_ENV=
FORCE_PRODUCTION=